{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-ansi",
                "-pedantic-errors",
                "-Wall",
                "-Wextra",
                "${file}",
                "/home/eden/eden-atar/ds/src/avl.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I",
                "/home/eden/eden-atar/ds/include"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "compiler: /usr/bin/gcc"
        }
    ]
}